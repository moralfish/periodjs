{
  "name": "periodjs",
  "version": "0.0.0-dev",
  "description": "Complex dates period comparisons",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:errors": "eslint ./src/**/*.{ts,tsx,js} --quiet && tsc --noEmit -p .",
    "lint:fixAll": "eslint ./src/**/*.{ts,tsx,js} --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "pre-release": "npm run lint && npm test",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moralfish/periodjs.git"
  },
  "keywords": [
    "date",
    "dates",
    "time",
    "datetime",
    "comparison",
    "period",
    "interval",
    "hours",
    "minutes",
    "seconds",
    "schedule"
  ],
  "author": {
    "name": "Nedal",
    "email": "nedal.elghamry@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moralfish/periodjs/issues"
  },
  "homepage": "https://github.com/moralfish/periodjs#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
